        <h4 class="card-title"></h4>
        <h6 class="card-subtitle"></h6>

        <div class="tab-container">
            <ul class="nav nav-tabs" role="tablist">
                <li class="nav-item">
                    <a class="nav-link active" data-toggle="tab" href="#property" role="tab">Property</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="tab" href="#address" role="tab">Address</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="tab" href="#feature" role="tab">Features</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" data-toggle="tab" href="#gallery" role="tab">Galleries</a>
                </li>
                @*<li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#policy" role="tab">Policies</a>
                    </li>*@
            </ul>

            @using (Html.BeginForm("Create", "PropertyDetails", FormMethod.Post, new { enctype = "multipart/form-data" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <div class="tab-content">
                <div class="tab-pane active fade show" id="property" role="tabpanel">
                    Property
                    @Html.Partial("~/Views/Property/Partial/_Property.cshtml", Model)
                </div>
                <div class="tab-pane fade" id="address" role="tabpanel">
                    Address
                    @Html.Partial("~/Views/Property/Partial/_Address.cshtml", Model)
                </div>
                <div class="tab-pane fade" id="feature" role="tabpanel">
                    feature
                    @Html.Partial("~/Views/Property/Partial/_FacilityList.cshtml", Model)
                </div>
                <div class="tab-pane fade" id="gallery" role="tabpanel">
                    gallery
                    @Html.Partial("~/Views/Property/Partial/_Gallery.cshtml", Model)
                </div>
                @*<div class="tab-pane fade" id="policy" role="tabpanel">
            @Html.Partial("~/Views/Property/Partial/_Gallery.cshtml", Model)
        </div>*@
